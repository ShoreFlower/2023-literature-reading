  booktitle                  = {2020 EUROPEAN CONTROL CONFERENCE (ECC 2020)},
  title                      = {Prescribed-Time Stabilization of Nonlinear Systems with Uncertain Input Gain and Non-Vanishing Disturbances},
  year                       = {2020},
  address                    = {345 E 47TH ST, NEW YORK, NY 10017 USA},
  note                       = {18th European Control Conference (ECC), ITMO Univ St Petersburg, ELECTR NETWORK, MAY 12-15, 2020},
  organization               = {European Control Assoc; Int Federat Automat Control; IEEE CSS; MathWorks; Processes},
  pages                      = {1859-1864},
  publisher                  = {IEEE},
  abstract                   = {A partial-state-feedback prescribed-time stabilizing controller is
   designed for a class of nonlinear systems with uncertain input gain,
   nonlinear uncertainties throughout the system dynamics involving both
   parametric and functional uncertainties, and time-varying non-vanishing
   disturbances generated by an appended dynamics. A dynamic adaptive
   controller is designed using a temporally transformed system
   representation in combination with a dynamic high-gain scaling based
   design procedure, and guarantees convergence of system state to the
   origin in the finite prescribed time.},
